{
  "name": "frontpage",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/frontpage",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project frontpage --web",
  "targets": {
    "build": {
      "command": "next build",
      "dependsOn": ["fetch-docs"]
    },
    "build-only": {
      "command": "next build"
    },
    "checks": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx typecheck frontpage",
          "nx lint frontpage"
        ],
        "parallel": true
      }
    },
    "fetch-docs": {
      "command": "tsx --tsconfig apps/frontpage/tsconfig.json apps/frontpage/scripts/getLocalDocs.ts"
    },
    "typecheck": {
      "command": "tsc --noEmit -p apps/frontpage/tsconfig.json"
    }
  }
}
